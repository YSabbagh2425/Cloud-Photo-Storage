<%- include('../partials/header') %>

 <h2>Photo List</h2>
 <table>
   <thead>
     <tr>
       <th>Title</th>
       <th>Description</th>
       <th>Data</th>
     </tr>
   </thead>
   <tbody>
      <% photos.forEach(function(p) { %>
       <tr>
         <td><%= p.title %></td>
         <td><%= p.desc %></td>
         <td><%= p.data %></td>
       </tr>
      <% }) %>
   </tbody>
 </table>

 <%- include('../partials/footer') %>
